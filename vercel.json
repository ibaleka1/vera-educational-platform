{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/app",
      "dest": "/public/app.html"
    },
    {
      "src": "/pricing",
      "dest": "/public/pricing.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
