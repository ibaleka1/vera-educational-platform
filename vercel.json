{
  "version": 2,
  "functions": {
    "api/chat.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chat.js"
    },
    {
      "src": "/app",
      "dest": "/public/app.html"
    },
    {
      "src": "/pricing",
      "dest": "/public/pricing.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
